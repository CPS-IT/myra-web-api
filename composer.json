{
    "name":"cpsit/myra-web-api",
    "description":"Php implementation of the myracloud.com api",
    "keywords":[
        "myracloud",
        "api"
    ],
    "homepage":"https://myrasecurity.com",
    "type":"library",
    "license":"MIT",
    "minimum-stability":"stable",
    "require":{
        "php":"^8.1 || ^8.2",
        "ext-json":"*",
        "guzzlehttp/guzzle":"^7.8",
        "symfony/console":"^6.4",
        "s1lentium/iptools":"^1.2",
        "symfony/yaml":"^7.0"
    },
	"require-dev":{
		"phpunit/phpunit": "^10"
	},
    "autoload":{
        "psr-4":{
            "Myracloud\\WebApi\\":"src/"
        }
    },
    "autoload-dev":{
        "psr-4":{
            "Myracloud\\Tests\\":"tests/"
        }
    }
}
